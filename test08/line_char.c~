#include <stdio.h>

#define max_length 1000

int main(void) {
    int array[max_length];
    int c = getchar();
    int i = 0;
    while (c != '\n') {
        c = getchar();
        array[i] = c;
        i++;
    }
    array[i] = '\0';
    int n = 0;
    scanf("%d",&n);
    printf("The character in position %d is '%c'\n", n, array[n]);
    return 0;
}
    
    
