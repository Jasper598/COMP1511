#include <stdio.h>
#include<string.h>

#define MAX_NUMBERS 1000000

int main(void) {
    int x[MAX_NUMBERS], i, j;
    printf("Enter numbers forwards:\n");
    int strlen(x) 
    i = 0;
    while (i < strlen[x]) {
        scanf("%d", &x[i]);
        i = i + 1;
    }
    printf("Numbers reversed are:\n");

    j = strlen[x] - 1;
    while (j >= 0) {
        printf("%d\n", x[j]);
        j = j - 1;
    }
    return 0;
}    
         
