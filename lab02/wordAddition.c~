#include <stdio.h>
int main(void){
    int x,y,sum;
    printf("Please enter two integers: ");
    scanf("%d,%d",&x,&y);
    sum=x+y;
    if(x==1){
    printf("one + ");
    }
        if(x==2){
    printf("two + ");
    }
        if(x==3){
    printf("three + ");
    }
        if(x==4){
    printf("four + ");
    }
        if(x==6){
    printf("six + ");
    }
        if(x==5){
    printf("five + ");
    }
        if(x==7){
    printf("seven + ");
    }
        if(x==8){
    printf("eight + ");
    }
        if(x==9){
    printf("nine + ");
    }
        if(x==10){
    printf("ten + ");
    }
        if(x==-1){
    printf("negative one + ");
    }
        if(x==-2){
    printf("negative two + ");
    }
        if(x==-3){
    printf("negative three + ");
    }
        if(x==-4){
    printf("negative four + ");
    }
        if(x==-6){
    printf("negative six + ");
    }
        if(x==-5){
    printf("negative five + ");
    }
        if(x==-7){
    printf("negative seven + ");
    }
        if(x==-8){
    printf("negative eight + ");
    }
        if(x==-9){
    printf("negative nine + ");
    }
        if(x==-10){
    printf("negative ten + ");
    }
        if(y==1){
    printf("one");
    }
            if(y==2){
    printf("two");
    }
            if(y==3){
    printf("three");
    }
            if(y==4){
    printf("four");
    }
            if(y==5){
    printf("five");
    }
            if(y==6){
    printf("six");
    }
            if(y==7){
    printf("seven");
    }
            if(y==8){
    printf("eight");
    }
            if(y==9){
    printf("nine");
    }
            if(y==10){
    printf("ten");
    }
    printf("%d\n",sum);
    return 0;
    }
